<?xml version="1.0" encoding="UTF-8"?>
<odoo>
<!--  VISTA DE FORMULARIO -->
    <record model="ir.ui.view" id="project_risk_impact_value_form_view">
        <field name="name">project.project.risk.form.view</field>
        <field name="model">project.project</field>
        <field name="inherit_id" ref="project.edit_project" />
        <field name="arch" type="xml">
            <page name="settings" position="before">
                <page name="risks_expect" string="Risks and Expectations">
                   <group name="risk_table"
                          string="Risk Table">
                       <field name="risk_table_ids"  nolabel="1"/>
                   </group>
                </page>
            </page>
        </field>
    </record>
</odoo>
